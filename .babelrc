{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": "11",
          "ie_mob": "11",
          "ff": "40",
          "chrome": "46",
          "safari": "8",
          "ios": "8",
          "android": "4.4"
        },
        "loose": true,
        "modules": false,
        "useBuiltIns": "entry",
        "debug": false
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env"
        ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-object-assign"
  ]
}